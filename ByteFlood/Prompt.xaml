<Window x:Class="ftorrent.Prompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Prompt" Height="100" Width="300" ResizeMode="NoResize">
    <Grid Height="54">
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,23,0,0" Name="textBox1" VerticalAlignment="Top" Width="254" TextChanged="textBox1_TextChanged" KeyDown="textBox1_KeyDown" />
        <Label Content="Enter magnet link:" Height="36" HorizontalAlignment="Left" Margin="12,0,0,0" Name="label1" VerticalAlignment="Top" />
    </Grid>
</Window>
